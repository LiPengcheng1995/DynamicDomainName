<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="online.dachuangdemo.www.DynamicDomainName.dao.MappingDao">
    <resultMap type="online.dachuangdemo.www.DynamicDomainName.domain.MappingVO" id="MappingVO">
        <id column="id" property="id"/>
        <result column="ip" property="ip" javaType="String"/>
        <result column="domainName" property="domainName"  javaType="String"/>
        <result column="description" property="description"  javaType="String"/>
        <result column="ifActive" property="ifActive" javaType="Boolean"/>
    </resultMap>

    <select id="getAllMappings" resultMap="MappingVO">
        select * from mapping
    </select>



</mapper>